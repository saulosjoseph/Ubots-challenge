{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ubots-challenge/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ubots-challenge/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ubots-challenge",
  "projects": {
    "ubots-challenge": {
      "type": "application",
      "root": "apps/ubots-challenge",
      "entryFile": "main",
      "sourceRoot": "apps/ubots-challenge/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ubots-challenge/tsconfig.app.json"
      }
    },
    "cards": {
      "type": "application",
      "root": "apps/cards",
      "entryFile": "main",
      "sourceRoot": "apps/cards/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cards/tsconfig.app.json"
      }
    }
  }
}